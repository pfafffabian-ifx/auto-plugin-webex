{
  "name": "auto-webex-plugin",
  "version": "0.4.2",
  "main": "dist/index.js",
  "description": "Webex plugin for auto",
  "license": "MIT",
  "author": {
    "name": "Fabian Pfaff",
    "email": "fabian.pfaff@infineon.com"
  },
  "publishConfig": {
